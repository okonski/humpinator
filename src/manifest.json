{
  "name": "Humpinator",
  "version": "0.5.0",
  "description": "Augment my Hump!",
  "homepage_url": "http://nfohump.com/forum/viewtopic.php?t=75547",
  "minimum_chrome_version" : "14.0",
  "manifest_version": 2,
  "update_url": "http://humpinator.nonidem.net/update/chrome",
  "icons": {
    "16": "icon.ico"
  },
  "web_accessible_resources": [
    "inject_quick_reply.js",
    "css/humpinator.css"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.nfohump.com/forum/index.php","http://www.nfohump.com/forum/viewforum.php*", "http://www.nfohump.com/forum/search.php*", "http://www.nfohump.com/forum/viewtopic.php*","http://www.nfohump.com/forum/posting.php?mode=reply*"],
      "js": [
        "jquery.min.js",
        "jquery.imagesloaded.min.js",
        "utils.js",
        "menu.js",
        "humpinator.js"
      ],
      "run_at" : "document_end"
    },
    {
      "matches": ["http://www.nfohump.com/forum/*"],
      "css": ["css/humpinator.css"],
      "run_at" : "document_end"
    }
  ],
  "permissions": [
    "http://www.nfohump.com/"
  ]
}